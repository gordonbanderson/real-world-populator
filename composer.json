{
    "name": "suilven/real-world-populator",
    "type": "library",
    "description": "Popuate your dev site with real world data",
    "keywords": [
        "suilven",
        "real-world-populator"
    ],
    "homepage": "https://github.com/suilven/real-world-populator",
    "license": "MIT",
    "authors": [
        {
            "name": "Gordon Anderson",
            "email": "gordon.b.anderson@gmail.com",
            "homepage": "https://github.com/gordonbanderson",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3",
        "html2text/html2text": "^4",
        "nesbot/carbon": "^1",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Suilven\\RealWorldPopulator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Suilven\\RealWorldPopulator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
